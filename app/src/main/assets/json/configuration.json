{
  "settings": {
    "screenshots_enabled": true,
    "toolbar_visible": true
  },
  "rules": [
    {
      "patterns": [
        ".*"
      ],
      "properties": {
        "context": "default",
        "uri": "hotwire://fragment/web",
        "pull_to_refresh_enabled": true
      }
    },
    {
      "patterns": [
        "/feed$",
        "/$"
      ],
      "properties": {
        "context": "default",
        "uri": "hotwire://fragment/web/without-toolbar",
        "pull_to_refresh_enabled": true,
        "toolbar_visible": false
      }
    },
    {
      "patterns": [
        "/settings$"
      ],
      "properties": {
        "context": "default",
        "uri": "hotwire://fragment/web",
        "pull_to_refresh_enabled": false,
        "toolbar_visible": true
      }
    }
  ]
}

